{
  "name": "@font-subseter/core",
  "version": "1.0.0",
  "description": "字体子集化核心库",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "font",
    "subset",
    "opentype",
    "woff",
    "woff2",
    "ttf",
    "otf"
  ],
  "author": "uaio",
  "license": "MIT",
  "devDependencies": {
    "@types/opentype.js": "^1.3.8",
    "@types/pako": "^2.0.4",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "fontkit": "^2.0.4",
    "opentype.js": "^1.3.4",
    "pako": "^2.1.0"
  }
}